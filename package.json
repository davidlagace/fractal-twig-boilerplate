{
    "name": "Fractal-Twig-webpack",
    "version": "0.1.0",
    "description": "This is a fractal boilerplate, using Bootstrap, Twig and sass.",
    "scripts": {
        "webpack:version": "webpack --v",
        "webpack:build": "webpack --mode=production --env.production",
        "webpack:dev": "webpack --mode=development --env.development",
        "webpack:watch": "webpack --mode=development --env.development --watch",
        "fractal:dev": "webpack --mode=development --env.development --watch & ./node_modules/.bin/fractal start --sync",
        "fractal:build": "webpack --mode=production --env.production & ./node_modules/.bin/fractal build"
    },
    "dependencies": {
        "@frctl/consolidate": "^1.0.1",
        "@frctl/fractal": "^1.0.8",
        "twig": "^0.9.5"
    },
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "devDependencies": {
        "autoprefixer": "^9.3.1",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "bootstrap": "^4.1.3",
        "css-loader": "^1.0.1",
        "mini-css-extract-plugin": "^0.4.4",
        "node-sass": "^4.9.4",
        "node-sass-glob-importer": "^5.2.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "path": "^0.12.7",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "url-loader": "^1.1.2",
        "webpack": "^4.23.1",
        "webpack-cli": "^3.1.2"
    }
}
